{
  "model": {
    "type": "minecraft:select",
    "cases": [
      {
        "when": "mizu_wand",
        "model": {
          "type": "minecraft:range_dispatch",
          "scale": 1,
          "index": 0,
          "entries": [
            {
              "threshold": 1,
              "model": {
                "type": "minecraft:model",
                "model": "item/mizu_wand_mana"
              }
            },
            {
              "threshold": 2,
              "model": {
                "type": "minecraft:model",
                "model": "item/mizu_wand_fire"
              }
            },
            {
              "threshold": 3,
              "model": {
                "type": "minecraft:model",
                "model": "item/mizu_wand_water"
              }
            },
            {
              "threshold": 4,
              "model": {
                "type": "minecraft:model",
                "model": "item/mizu_wand_ender"
              }
            }
          ],
          "property": "custom_model_data",
          "fallback": {
            "type": "minecraft:model",
            "model": "item/mizu_wand"
          }
        }
      }
    ],
    "property": "custom_model_data",
    "fallback": {
      "type": "minecraft:model",
      "model": "item/stone_pickaxe"
    }
  }
}